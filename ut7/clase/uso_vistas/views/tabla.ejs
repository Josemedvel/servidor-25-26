<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    
</head>
<body>
    <%- include("partials/header.ejs") %>
    <h1>Tabla de nombres</h1>
    <table border="1px solid black">
        <tr>
            <th>Nombres</th>
        </tr>
        <%
            nombres.forEach(element => {
        %>    
        <tr>
            <td>
                <%= element%>        
            </td>
        </tr>
    
        <%})
        %>
    </table>

</body>
</html>